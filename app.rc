// Windows 资源文件
// 用于设置应用程序图标和版本信息

#include <windows.h>

// 应用程序图标（如果有 app.ico 文件）
IDI_ICON1 ICON "app.ico"

// 版本信息
VS_VERSION_INFO VERSIONINFO
FILEVERSION     0,1,0,0
PRODUCTVERSION  0,1,0,0
FILEFLAGSMASK   0x3fL
FILEFLAGS       0x0L
FILEOS          VOS_NT_WINDOWS32
FILETYPE        VFT_APP
FILESUBTYPE     VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "080404b0"  // 简体中文
        BEGIN
            VALUE "CompanyName", "iPhone Stock Management\0"
            VALUE "FileDescription", "iPhone 库存管理系统\0"
            VALUE "FileVersion", "0.1.0.0\0"
            VALUE "InternalName", "iPhoneStockManagement\0"
            VALUE "LegalCopyright", "Copyright (C) 2025\0"
            VALUE "OriginalFilename", "iPhoneStockManagement.exe\0"
            VALUE "ProductName", "iPhone 库存管理系统\0"
            VALUE "ProductVersion", "0.1.0.0\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x804, 1200  // 简体中文, Unicode
    END
END
